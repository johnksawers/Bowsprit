%h1 Start Interview
= form_for @interview, :method => :post, :url => question_set_interviews_path(@qs) do |f|
	-if @interview.errors.any?
		#error_explanation
			%h2= "#{pluralize(@interview.errors.count, "error")} prohibited this question from being saved:"
			%ul
			- @interview.errors.full_messages.each do |msg|
				%li= msg
	.fields
		= f.label :name
		= f.text_field :name
		= f.hidden_field :question_set_id
	.actions
		= f.submit 'Save and Begin Interview'
= link_to 'Back', root_path
